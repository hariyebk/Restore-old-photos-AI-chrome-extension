{
    "manifest_version": 3,
    "name": "Restore old photos",
    "version": "1.0",
    "description": "Restore old, damaged, or low-quality photos. With just a few clicks.",
    "icons": {
        "128": "128.png"
    },
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_icon": "128.png",
        "default_popup": "popup.html"
    },
    "permissions": ["storage", "downloads"]
}